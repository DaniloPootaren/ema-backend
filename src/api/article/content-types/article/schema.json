{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Product",
        "Body",
        "Health"
      ],
      "required": true
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "title": {
      "type": "string"
    }
  }
}
